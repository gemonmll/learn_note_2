{
  "version": "2.0.0",
  "tasks": [
      {
          "label": "start_carto",
          "type": "shell",
          "command": "bash -c 'source /opt/ros/noetic/setup.bash && source /workspace/catkin_ws/devel_isolated/cartographer_ros/setup.bash && roslaunch cartographer_ros demo_backpack_2d.launch bag_filename:=/workspace/catkin_ws/download/cartographer_paper_deutsches_museum.bag' ",
          "isBackground": true
      },
      {
          "label": "delay_3s",
          "type": "shell",
          "command": "sleep 3"
      },
      {
          "label": "source_ros2_env",
          "dependsOn": [
              "start_carto",
              "delay_3s"
          ],
          "dependsOrder": "sequence",
